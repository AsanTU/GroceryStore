<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<AnchorPane prefHeight="400.0" prefWidth="500.0" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="sample.grocerystore.controllers.NewArrivalsController">
    <TextField fx:id="nameField" layoutX="175.0" layoutY="44.0" promptText="Name" styleClass="text-field"/>
    <TextField fx:id="quantityField" layoutX="175.0" layoutY="126.0" promptText="Quantity" styleClass="text-field"/>
    <TextField fx:id="pricePerPeaceField" layoutX="175.0" layoutY="209.0" promptText="Price per peace"
               styleClass="text-field"/>
    <Button fx:id="addBtn" layoutX="200.0" layoutY="297.0" mnemonicParsing="false"
            onMouseClicked="#navigateToInventoryPanel" prefHeight="50" prefWidth="100" text="Add"/>
</AnchorPane>
